{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "env": {
    "production": {
      "plugins": [
        "emotion",
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-syntax-dynamic-import"
      ]
    },
    "development": {
      "plugins": [["emotion", { "sourceMap": true }], "react-docgen"]
    }
  }
}
