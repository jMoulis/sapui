{
  "entities": {
    "plants": {
      "label": "plants",
      "entity": "Plants",
      "displayField": "Name",
      "uri": "http://localhost:3000/plants.json",
      "initMenu": true
    },
    "products": {
      "label": "products",
      "entity": "Products",
      "displayField": "Name",
      "uri": "http://localhost:3000/products.json",
      "initMenu": true
    },
    "posts": {
      "label": "posts",
      "entity": "Posts",
      "displayField": "OrderID",
      "uri": "http://localhost:3000/plantsProductsPosts.json"
    }
  },
  "router": {
    "hedging": {
      "title": "Hedging",
      "allowed": true,
      "path": "/hedging",
      "component": "Hedging",
      "componentPath": "Hedging/Hedging",
      "routes": [
        {
          "title": "Plants",
          "path": "/hedging/plants",
          "uri": "/plants.json",
          "component": "HedgeContent",
          "componentPath": "Hedging/HedgeContent",
          "routes": [
            {
              "title": "Plants",
              "path": "/:id",
              "component": "HedgeContent",
              "componentPath": "Hedging/HedgeContent"
            }
          ]
        },
        {
          "title": "Products",
          "path": "/hedging/products",
          "uri": "/products.json",
          "component": "HedgeContent",
          "componentPath": "Hedging/HedgeContent"
        },
        {
          "title": "Posts",
          "path": "/hedging/plants/posts",
          "component": "HedgeContent",
          "componentPath": "Hedging/HedgeContent",
          "uri": "/plantsProductsPosts.json"
        }
      ]
    },
    "category": {
      "title": "Exo 1",
      "allowed": true,
      "path": "/category",
      "component": "Categories",
      "componentPath": "Categories/Category",
      "routes": [
        {
          "path": "/:category",
          "title": "Exercice 1",
          "allowed": true,
          "component": "CategoryDetail",
          "componentPath": "Categories/CategoryDetail/CategoryDetail"
        }
      ]
    },
    "app3": {
      "title": "App 3",
      "allowed": true,
      "exact": true,
      "path": "/app3",
      "component": "Appli3",
      "componentPath": "Appli3/Appli3",
      "routes": []
    }
  },
  "initNavMenu": ["plants", "products"]
}
